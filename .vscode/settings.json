{
    "nuxt.isNuxtApp": false,
    "css.validate": false,
    "tailwindCSS.emmetCompletions": true,
    "editor.inlineSuggest.enabled": true,
    "tailwindCSS.includeLanguages": {
        "plaintext": "html",
        "plaintext": "astro",
        "javascript":"javascript",
    },
    "files.associations": {
        "*.xml": "html",
        "*.svg": "html",
    },
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "tailwindCSS.classAttributes": [
        "class",
        "className",
        "ngClass"
    ],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#2c2c39",
        "activityBar.background": "#2c2c39",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#a73838",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#2c2c39",
        "statusBar.background": "#16161c",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#2c2c39",
        "statusBarItem.remoteBackground": "#16161c",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#16161c",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#16161c99",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#16161C"
}